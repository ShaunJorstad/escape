{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "escape",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "path": {
        "all": true
      },
      "fs": {
        "all": true,
        "scope": ["$APPDATA", "$APPDATA/*"]
      },
      "window": {
        "all": true,
        "startDragging": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.jorstad.escape",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://raw.githubusercontent.com/ShaunJorstad/escape/main/Release.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDQ4ODU5NkIxRTVBNTg2REMKUldUY2hxWGxzWmFGU0tsNytKS2laQ2tNZ25oek5wek44aERsekJqbzI0SDJhWTRIM2xiUW1IR0sK"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "",
        "width": 800,
        "height": 600,
        "titleBarStyle": "Overlay"
      }
    ]
  }
}
