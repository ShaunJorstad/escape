{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "escape",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "startDragging": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.jorstad.escape",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://gist.githubusercontent.com/ShaunJorstad/df4b0864630d2e141f9512a79ebbaa9f/raw"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEVDODdDMUE3MzlGM0YxNDQKUldSRThmTTVwOEdIN0pnOGdTQ1kxREZNcWNUOS8waCtoWE5iaGgwNXdxWHU2Q09GK2N6a29taHAK"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "",
        "width": 800,
        "height": 600,
        "titleBarStyle": "Overlay"
      }
    ]
  }
}
